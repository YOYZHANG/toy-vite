{
  "name": "toy-vite",
  "version": "1.0.0",
  "description": "",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "bin",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "bin": {
    "vds": "bin/vd.js"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "dev": "run-p dev-client dev-node",
    "dev-client": "tsc -w --incremental --p src/client",
    "dev-node": "tsc -w --incremental --p src/node",
    "build": "rimraf dist && tsc -p src/client && tsc -p src/node",
    "lint": "",
    "test": "jest --clearCache && jest --runInBand --forceExit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOYZHANG/toy-vite.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YOYZHANG/toy-vite/issues"
  },
  "homepage": "https://github.com/YOYZHANG/toy-vite#readme",
  "dependencies": {
    "@babel/parser": "^7.15.3",
    "chokidar": "^3.5.2",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "magic-string": "^0.25.7",
    "path": "^0.12.7",
    "serve-handler": "^6.1.3",
    "url": "^0.11.0",
    "ws": "^8.0.0"
  }
}
